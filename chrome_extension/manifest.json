{
    "name": "SayWhat",
    "version": "1.0",
    "description": "Real time captions for your video-conferencing calls",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_title": "SayWhat"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ],
    "manifest_version": 2
}
